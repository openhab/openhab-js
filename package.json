        "mocha": "^6.2.2",
        "proxyquire": "^2.1.3",
        "rewiremock": "^3.13.9",
        "snazzy": "^9.0.0",
        "standardx": "^7.0.0",
        "typescript": "^4.7.3",
        "webpack": "^5.58.2",
        "webpack-cli": "^4.9.1"
    }
